                                  "type": "object",
                                },
                                "type": [
                                  "array",
                                  "null",
                                ],
                              },
                            },
                            "required": [
                              "cfd",
                              "rncp",
                            ],
                            "type": "object",
                          },
                          "rome": {
                            "properties": {
                              "rncp": {
                                "description": "Code(s) et intitulé(s) de la (ou des) fiche(s) métier du Répertoire Opérationnel des Métiers et de l’Emploi (ROME) associé(s) à la certification.",
                                "items": {
                                  "properties": {
                                    "code": {
                                      "example": "D1102",
                                      "type": "string",
                                    },
                                    "intitule": {
                                      "example": "Boulangerie - viennoiserie",
                                      "type": "string",
                                    },
                                  },
                                  "required": [
                                    "code",
                                    "intitule",
                                  ],
                                  "type": "object",
                                },
                                "type": [
                                  "array",
                                  "null",
                                ],
                              },
                            },
                            "required": [
                              "rncp",
                            ],
                            "type": "object",
                          },
                        },
                        "required": [
                          "formacodes",
                          "nsf",
                          "rome",
                        ],
                        "type": "object",
                      },
